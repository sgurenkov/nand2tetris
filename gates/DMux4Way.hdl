/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    Not(in=sel[1], out=notSel2);
    And(a=notSel2, b=in, out=in1);
    And(a=sel[1], b=in, out=in2);
    DMux(in=in1, a=a, b=b, sel=sel[0]);
    DMux(in=in2, a=c, b=d, sel=sel[0]);
}
